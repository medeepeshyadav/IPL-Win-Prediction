{
  "manifest_version":2,
  "name":"myfirst_extention",
  "version":"0.001",
  "content_scripts":[
    {
      "matches":[
        "https://www.cricbuzz.com/live-cricket-scorecard/*"
      ],
      "js":["content.js"]
    }
  ],
  "permissions":["tabs"],
  "background":{
    "scripts":["background.js"]
  },
